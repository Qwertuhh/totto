{
  "$schema": "https://json.schemastore.org/package",
  "name": "aiticket",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run delete:dist && npm run build:frontend && npm run build:backend && npm run copy:dist",
    "build:frontend": "cd apps/frontend && npm run build",
    "build:backend": "cd apps/backend && npm run build",
    "copy:dist": "powershell -Command \"Copy-Item -Path apps/frontend/dist -Destination dist/frontend -Recurse; Copy-Item -Path apps/backend/dist -Destination dist/backend -Recurse\"",
    "delete:dist": "powershell -Command \"Remove-Item -Recurse -Force dist/\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^22.15.30",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.10",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "heroui-cli": "^1.2.3",
    "mongoose": "^8.16.0",
    "winston": "^3.17.0"
  }
}
